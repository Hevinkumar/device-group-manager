<div class="popup-model-content">
    {{!-- {{log this.displaydata}} --}}
    {{#each this.displaydata as |item|}}
    <div class="group-container bg-color-white {{if item.selected " selected"}} {{if this.display_overflow "min-wid" "max-wid"}}" onclick={{action "selectToggleItem" item}}>
        <i class="{{item.icon}} icon {{if item.selected " color-selected" "color-normal" }}"></i>
        <div class="group-component ">
            <div class="div-ellipsis {{if item.selected " color-selected" " color-normal" }}">
                <span class="tooltiptext-up">{{item.title}}</span>
                <span class="title " value="{{item.title}}">{{item.title}}</span>
            </div>
            <div class="div-ellipsis {{if item.selected " color-selected" " color-normal"}}">
                <span class="sub-title"><i class="fa-solid fa-mobile "></i> {{item.subtitle}}</span>
                <span class="tooltiptext-down">{{item.subtitle}}</span>
            </div>
        </div>
    </div>
    {{/each}}
    {{!-- {{log this.loading}} --}}
    {{#if this.loading}}
        <div class="loading">
            <img src="images/spinner.gif" alt="">
        </div>
    {{/if}}
</div>
{{yield}}